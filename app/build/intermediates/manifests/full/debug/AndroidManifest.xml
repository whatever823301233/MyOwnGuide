            android:name="com.systekcn.guide.activity.EmbeddedDrawerActivity"
            android:theme="@style/MaterialDrawerTheme" />
        <activity
            android:name="com.systekcn.guide.activity.MiniDrawerActivity"
            android:theme="@style/MaterialDrawerTheme" />
