            android:label="@string/app_name"
            android:theme="@style/CustomTheme" />
        <activity
            android:name="com.systekcn.guide.activity.PersistentDrawerActivity"
            android:label="@string/app_name"
            android:theme="@style/MaterialDrawerTheme.Light.TranslucentStatus" />
        <activity
            android:name="com.systekcn.guide.activity.ActionBarActivity"
            android:theme="@style/MaterialDrawerTheme.ActionBar" />
