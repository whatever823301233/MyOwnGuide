            android:name="com.systekcn.guide.activity.CollapsingToolbarActivity"
            android:theme="@style/MaterialDrawerTheme.Light.DarkToolbar.TranslucentStatus" />
        <activity
            android:name="com.systekcn.guide.activity.CrossfadeDrawerLayoutActvitiy"
            android:theme="@style/MaterialDrawerTheme" />
        <activity
            android:name="com.systekcn.guide.activity.BeginActivity"
            android:theme="@style/FullscreenTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.systekcn.guide.activity.CityChooseActivity"
            android:theme="@style/FullscreenTheme" >
