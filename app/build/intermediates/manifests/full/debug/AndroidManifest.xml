            android:name="com.systekcn.guide.activity.MenuDrawerActivity"
            android:theme="@style/MaterialDrawerTheme.Light.DarkToolbar.TranslucentStatus" />
        <activity
            android:name="com.systekcn.guide.activity.NonTranslucentDrawerActivity"
